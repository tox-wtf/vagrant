upstream = "gh:KhronosGroup/spirv-tools"

[[channels]]
name = "release"
fetch = '''gr | vtrim -re 's,^(vulkan(-sdk|)|sdk)-,,' | fsl | grep -Ev '[0-9]+\.[0-9]+\.[0-9]+\.[0-9]+' | vfs | vsort'''
expected = '''^[0-9]{4}.[0-9]$'''

[[channels]]
name = "sdk"
fetch = "gr | grep '^vulkan-sdk-' | cut -d- -f3- | vsort"
expected = '''^[0-9]+\.[0-9]+\.[0-9]+\.[0-9]+$'''

[[channels]]
name = "commit"
